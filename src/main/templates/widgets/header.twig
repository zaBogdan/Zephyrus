{% block header %}

<head>
  <meta charset="utf-8">
  <meta content="width=device-width, initial-scale=1.0" name="viewport" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
  <title>{{ header.root | default('Zephyrus Dev')}} - {{ header.title | capitalize }}</title>
  <link rel="icon" type="image/png" href="/main/style/img/logo.svg">
  <link rel="apple-touch-icon" sizes="76x76" href="/main/style/img/logo.svg">
  <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" />
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css">
  <link href="/main/style/css/material-kit.css?v=2.0.7" rel="stylesheet" />
  {% if header.editor == true %}
  <script src="https://cdn.tiny.cloud/1/{{env.TINYMCE_KEY}}/tinymce/5/tinymce.min.js" referrerpolicy="origin"></script>
  {% endif %}
</head>

{% endblock %}